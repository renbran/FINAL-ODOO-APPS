<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
    <!-- Odoo XML Schema Catalog for VS Code -->
    <!-- This helps VS Code understand Odoo XML structure -->

    <!-- Odoo base elements -->
    <public publicId="-//ODOO//DTD Odoo Views//EN" uri="odoo-views.xsd"/>
    <public publicId="-//ODOO//DTD Odoo Data//EN" uri="odoo-data.xsd"/>
    <public publicId="-//ODOO//DTD Odoo Security//EN" uri="odoo-security.xsd"/>

    <!-- Common Odoo namespaces -->
    <uri name="http://www.w3.org/1999/xhtml" uri="xhtml.xsd"/>
    <uri name="http://www.w3.org/2001/XMLSchema-instance" uri="xsi.xsd"/>
</catalog>
