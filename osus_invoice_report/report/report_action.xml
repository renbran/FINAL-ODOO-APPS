<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- OSUS Invoice Report Action -->
        <report
            id="action_report_osus_invoice"
            model="account.move"
            string="OSUS Invoice"
            report_type="qweb-pdf"
            name="osus_invoice_report.report_osus_invoice_document"
            file="osus_invoice_report.report_osus_invoice_document"
            print_report_name="'OSUS-Invoice-%s' % (object.name)"
            paperformat="base.paperformat_euro"
        />

        <!-- OSUS Bill Report Action -->
        <report
            id="action_report_osus_bill"
            model="account.move"
            string="OSUS Bill"
            report_type="qweb-pdf"
            name="osus_invoice_report.report_osus_bill_document"
            file="osus_invoice_report.report_osus_bill_document"
            print_report_name="'OSUS-Bill-%s' % (object.name)"
            paperformat="base.paperformat_euro"
        />
    </data>
</odoo>