<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- External Commission Sequence -->
        <record id="seq_external_commission" model="ir.sequence">
            <field name="name">External Commission</field>
            <field name="code">external.commission</field>
            <field name="prefix">EXT-COM/</field>
            <field name="suffix"></field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>

        <!-- Internal Commission Sequence -->
        <record id="seq_internal_commission" model="ir.sequence">
            <field name="name">Internal Commission</field>
            <field name="code">internal.commission</field>
            <field name="prefix">INT-COM/</field>
            <field name="suffix"></field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>

        <!-- Commission Purchase Order Sequence (if needed) -->
        <record id="seq_commission_purchase" model="ir.sequence">
            <field name="name">Commission Purchase Order</field>
            <field name="code">commission.purchase.order</field>
            <field name="prefix">COM-PO/</field>
            <field name="suffix"></field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>