<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron job for automatic commission processing -->
        <record id="cron_auto_process_commissions" model="ir.cron">
            <field name="name">Auto Process Commissions</field>
            <field name="model_id" ref="sale.model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_auto_process_commissions()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
            <field name="priority">5</field>
            <field name="user_id" ref="base.user_admin"/>
            <field name="doall">False</field>
        </record>
    </data>
</odoo>
