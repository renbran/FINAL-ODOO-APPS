<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Property Sequence -->
        <record id="seq_property_property" model="ir.sequence">
            <field name="name">Property Sequence</field>
            <field name="code">property.property</field>
            <field name="prefix">PROP/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Property Offer Sequence -->
        <record id="seq_property_offer" model="ir.sequence">
            <field name="name">Property Offer Sequence</field>
            <field name="code">property.offer</field>
            <field name="prefix">OFFER/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Property Sale Sequence -->
        <record id="seq_property_sale" model="ir.sequence">
            <field name="name">Property Sale Sequence</field>
            <field name="code">property.sale</field>
            <field name="prefix">SALE/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Broker Commission Sequence -->
        <record id="seq_broker_commission" model="ir.sequence">
            <field name="name">Broker Commission Sequence</field>
            <field name="code">broker.commission.invoice</field>
            <field name="prefix">BCOM/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Internal Commission Sequence -->
        <record id="seq_internal_commission" model="ir.sequence">
            <field name="name">Internal Commission Sequence</field>
            <field name="code">internal.commission</field>
            <field name="prefix">ICOM/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>