<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_commission_analysis" model="ir.actions.act_window">
        <field name="name">Commission Analysis</field>
        <field name="res_model">sale.order</field>
        <field name="view_mode">pivot,tree,graph,form</field>
        <field name="domain">[('commission_status','!=','draft')]</field>
        <field name="context">{
            'search_default_group_by_project':1,
            'search_default_group_by_status':1,
            'search_default_filter_commission':1
        }</field>
    </record>
</odoo>