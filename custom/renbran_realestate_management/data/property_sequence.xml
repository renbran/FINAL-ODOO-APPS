<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <!-- Sequence for Property References -->
    <record id="sequence_property" model="ir.sequence">
        <field name="name">Property Sequence</field>
        <field name="code">property.property</field>
        <field name="prefix">PROP/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <!-- Sequence for Offer References -->
    <record id="sequence_property_offer" model="ir.sequence">
        <field name="name">Property Offer Sequence</field>
        <field name="code">property.offer</field>
        <field name="prefix">OFFER/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <!-- Sequence for Sale References -->
    <record id="sequence_property_sale" model="ir.sequence">
        <field name="name">Property Sale Sequence</field>
        <field name="code">property.sale</field>
        <field name="prefix">SALE/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <!-- Sequence for Broker Commission References -->
    <record id="sequence_broker_commission" model="ir.sequence">
        <field name="name">Broker Commission Sequence</field>
        <field name="code">broker.commission.invoice</field>
        <field name="prefix">BCOM/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <!-- Sequence for Internal Commission References -->
    <record id="sequence_internal_commission" model="ir.sequence">
        <field name="name">Internal Commission Sequence</field>
        <field name="code">internal.commission</field>
        <field name="prefix">ICOM/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
