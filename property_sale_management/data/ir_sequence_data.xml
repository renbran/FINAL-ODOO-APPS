<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Property Sequence -->
        <record id="seq_property_reference" model="ir.sequence">
            <field name="name">Property Reference</field>
            <field name="code">property.reference</field>
            <field name="prefix">PROP/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Property sale Sequence -->
        <record id="property_sale_sequence" model="ir.sequence">
            <field name="name">Property Sale Sequence</field>
            <field name="code">property.sale</field>
            <field name="active">TRUE</field>
            <field name="prefix">PS/</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
