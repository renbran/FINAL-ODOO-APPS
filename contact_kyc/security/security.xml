<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- KYC Manager Group -->
        <record id="group_kyc_manager" model="res.groups">
            <field name="name">KYC Manager</field>
            <field name="category_id" ref="base.module_category_administration"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
        </record>

        <!-- KYC User Group -->
        <record id="group_kyc_user" model="res.groups">
            <field name="name">KYC User</field>
            <field name="category_id" ref="base.module_category_administration"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

    </data>
</odoo>
