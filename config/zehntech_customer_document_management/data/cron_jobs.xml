<odoo>
   

    <record id="cron_document_expiry_notification" model="ir.cron">
        <field name="name">Document Expiry Notification</field>
        <field name="model_id" ref="model_res_partner_document" />
        <field name="state">code</field>
        <field name="code">model.send_expiry_notification()</field>
        <field name="interval_type">days</field>
        <field name="interval_number">1</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True" />
    </record>
    

    <record id="ir_cron_update_document_dashboard" model="ir.cron">
        <field name="name">Update Document Expiry Dashboard</field>
        <field name="model_id" ref="model_customer_document_dashboard" />
        <field name="state">code</field>
        <field name="code">model.calculate_expiry_data()</field>
        <field name="interval_number">1</field>  
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>



   
</odoo>
