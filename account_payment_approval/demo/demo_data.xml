    <record id="demo_payment_maintenance_repair" model="account.payment">
        <field name="partner_id" ref="demo_partner_maintenance_company"/>
        <field name="payment_type">outbound</field>
        <field name="amount">2500.00</field>
        <field name="ref">Monthly maintenance for Property Complex A</field>
        <field name="date" eval="(DateTime.now() - timedelta(days=5)).strftime('%Y-%m-%d')"/>
        <field name="voucher_state">submitted</field>
        <field name="voucher_number">PV-2024-001</field>
        <field name="related_document_type">Invoice</field>
        <field name="related_document_number">INV-MAINT-2024-001</field>
        <field name="related_document_date" eval="(DateTime.now() - timedelta(days=7)).strftime('%Y-%m-%d')"/>
        <field name="amount_in_words">Two Thousand Five Hundred Dollars and Zero Cents</field>
        <field name="creator_name">System Administrator</field>
        <field name="creator_signature_date" eval="(DateTime.now() - timedelta(days=5)).strftime('%Y-%m-%d %H:%M:%S')"/>
    </record>

