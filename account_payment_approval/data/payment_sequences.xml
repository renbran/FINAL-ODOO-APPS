<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Payment Voucher Sequence -->
    <record id="sequence_payment_voucher" model="ir.sequence">
        <field name="name">Payment Voucher Sequence</field>
        <field name="code">payment.voucher.sequence</field>
        <field name="prefix">PV</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Receipt Voucher Sequence -->
    <record id="sequence_receipt_voucher" model="ir.sequence">
        <field name="name">Receipt Voucher Sequence</field>
        <field name="code">receipt.voucher.sequence</field>
        <field name="prefix">RV</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Transfer Voucher Sequence -->
    <record id="sequence_transfer_voucher" model="ir.sequence">
        <field name="name">Transfer Voucher Sequence</field>
        <field name="code">transfer.voucher.sequence</field>
        <field name="prefix">TV</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
