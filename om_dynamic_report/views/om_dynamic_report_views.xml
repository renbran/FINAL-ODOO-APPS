<odoo>
    <record id="view_om_dynamic_report_tree" model="ir.ui.view">
        <field name="name">om.dynamic.report.tree</field>
        <field name="model">om.dynamic.report</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="report_type"/>
                <field name="model_id"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="view_om_dynamic_report_form" model="ir.ui.view">
        <field name="name">om.dynamic.report.form</field>
        <field name="model">om.dynamic.report</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="report_type"/>
                        <field name="model_id"/>
                        <field name="filter_domain"/>
                        <field name="active"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
